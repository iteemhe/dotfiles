{
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.intelliSenseEngine": "Disabled",
  "Lua.hint.enable": true,
  "Lua.telemetry.enable": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "better-comments.highlightPlainText": true,
  "clangd.arguments": [
    "-j=16",
    "--clang-tidy",
    "--clang-tidy-checks=*",
    "--all-scopes-completion",
    "--completion-style=detailed",
    "--header-insertion-decorators",
    "--header-insertion=iwyu",
    "--pch-storage=memory"
  ],
  "cmake.configureOnOpen": true,
  "cmake.generator": "Unix Makefiles",
  "cmake.revealLog": "never",
  "codesnap.shutterAction": "copy",
  "codesnap.transparentBackground": true,
  "customizeUI.font.monospace": "SauceCodePro Nerd Font Mono",
  "customizeUI.font.regular": "SauceCodePro Nerd Font Mono",
  "customizeUI.titleBar": "frameless",
  "debug.console.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
  "debug.console.fontSize": 24,
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.wordWrap": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "block",
  "editor.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
  "editor.fontSize": 24,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": true,
  "editor.linkedEditing": true,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.wordWrap": "off",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "java.debug.settings.showStaticVariables": true,
  "java.import.gradle.java.home": "/usr/local/Cellar/openjdk@11/11.0.12/libexec/openjdk.jdk/Contents/Home",
  "java.project.referencedLibraries": ["lib/**/*.jar"],
  "json.maxItemsComputed": 10000,
  "jupyter.askForKernelRestart": false,
  "kotlin.compiler.jvm.target": "\"11\"",
  "kotlin.languageServer.path": "/usr/local/bin/kotlin-language-server",
  "latex-workshop.view.pdf.viewer": "tab",
  "liveshare.autoShareServers": false,
  "liveshare.autoShareTerminals": false,
  "liveshare.shareExternalFiles": false,
  "lldb.showDisassembly": "never",
  "materialTheme.accent": "Acid Lime",
  "notebook.lineNumbers": "on",
  "oneDarkPro.bold": true,
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  "oneDarkPro.vivid": true,
  "python.defaultInterpreterPath": "/usr/local/Caskroom/miniconda/base/bin/python",
  "python.formatting.blackArgs": ["--line-length=100"],
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.assist.importEnforceGranularity": true,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.debug.openDebugPane": true,
  "rust-analyzer.diagnostics.enableExperimental": true,
  "rust-analyzer.hoverActions.references": true,
  "rust-analyzer.inlayHints.closureReturnTypeHints": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
  "rust-analyzer.inlayHints.maxLength": null,
  "rust-analyzer.inlayHints.reborrowHints": true,
  "rust-analyzer.lens.enumVariantReferences": true,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.lens.references": true,
  "rust-analyzer.lruCapacity": 128,
  "rust-analyzer.primeCaches.numThreads": 16,
  "rust-analyzer.rustfmt.enableRangeFormatting": true,
  "rust-analyzer.workspace.symbol.search.kind": "all_symbols",
  "rust-analyzer.workspace.symbol.search.scope": "workspace_and_dependencies",
  "rust-analyzer.server.path": "~/.rustup/toolchains/nightly-x86_64-apple-darwin/bin/rust-analyzer",
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 24,
  "terminal.integrated.inheritEnv": false,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.smartRelativeLine": true,
  "vim.timeout": 200,
  "vim.vimrc.enable": true,
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "window.openFoldersInNewWindow": "on",
  "window.titleBarStyle": "native",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.localHistory.maxFileEntries": 2000,
  "workbench.localHistory.maxFileSize": 1073741824,
  "window.zoomLevel": 1,
  "customizeUI.statusBarPosition": "under-panel",
  "clangd.path": "/usr/local/bin/clangd",
  "cSpell.caseSensitive": true,
  "customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "gradle.allowParallelRun": true
}
